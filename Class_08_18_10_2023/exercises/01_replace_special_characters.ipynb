   "source": [
    "Write a program to replace all special characters from a string."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "from string import punctuation, digits\n",
    "\n",
    "text = input(\"Enter String:\")\n",
    "\n",
    "for i in punctuation + digits:\n",
    "    text = text.replace(i, \"\")\n",
    "\n",
    "print(text)"
   ]
  }
